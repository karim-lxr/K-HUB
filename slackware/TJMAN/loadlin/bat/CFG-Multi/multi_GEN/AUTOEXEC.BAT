goto %config%
:Normale
C:\DOS\SMARTDRV.EXE /X
@ECHO OFF
PROMPT $P$G
PATH=C:\;C:\WINDOWS;C:\DOS
SET LMOUSE=C:\MOUSE
C:\MOUSE\MOUSE.EXE
SET TEMP=C:\temp
REM MODE CON CODEPAGE PREPARE=((437) C:\DOS\EGA.CPI)
MODE CON CODEPAGE PREPARE=(437) C:\DOS\EGA.CPI  
MODE CON CODEPAGE SELECT=437
KEYB SG,,C:\DOS\KEYBOARD.SYS
goto fine


:CDROM
C:\DOS\SMARTDRV.EXE /X
@ECHO OFF
PROMPT $P$G
PATH=C:\;C:\WINDOWS;C:\DOS
SET LMOUSE=C:\MOUSE
C:\MOUSE\MOUSE.EXE
SET TEMP=C:\temp
REM MODE CON CODEPAGE PREPARE=((437) C:\DOS\EGA.CPI)
