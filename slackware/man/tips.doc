\par\pard\plain\ltrpar\ql\s44\sl240\slmult1\itap0\tx0\tx958\tx1917\tx2877\tx3836\tx4794\tx5753\tx6713\tx7672\tx8631{\f2\fs20\lang1033{\*\listtag0}\abinodiroverride\ltrch (sleep 2; echo -e "user\\r" ; sleep 1; echo -e "pass\\r"; ) | netcat -t nomehost 23}
